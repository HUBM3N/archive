"VertexLitGeneric"
{
	"$basetexture"	"models/workshop_partner/weapons/c_models/c_bet_rocketlauncher/c_bet_rocketlauncher"
	"$basemapalphaphongmask" "1"

	//Phong
	"$phong" "1"
	"$phongexponentfactor" "25"
	"$phongboost" "10"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 20]"
	"$halflambert" "1"
	
	//Rim Light
	"$rimlight" "1"					
	"$rimlightexponent" "4"		
	"$rimlightboost" "2"

	//Detail (Gradient overlayed)
	"$detail" "models/workshop_partner/weapons/c_models/c_bet_rocketlauncher/c_bet_rocketlauncher_gradient"
	"$detailblendfactor" "0.75"
	"$detailblendmode" "0"
	"$detailscale" "1"

	//Cloaking
	"$cloakPassEnabled" "1"

	//Weapon Sheens
	"$sheenPassEnabled" "1"
	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"
	//Crit Glow
	"$glowcolor" "1"
	//Jarate
	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}